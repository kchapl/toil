GET         /assets/*file               controllers.Assets.at(path="/public", file)

GET         /                           controllers.DashboardController.view()

GET         /oauth2callback             controllers.AuthController.authCallback(code,state)

GET         /accounts                   controllers.AccountController.viewAccounts()
GET         /account/:name              controllers.AccountController.viewAccount(name)

GET         /transactions               controllers.TransactionController.viewTransactions()
GET         /transactions/upload        controllers.TransactionController.viewUploadTransactions()
POST        /transactions/upload        controllers.TransactionController.uploadTransactions()

GET         /surplus                    controllers.SurplusController.viewSurplus()
GET         /surplusFigs                controllers.SurplusController.viewSurplusFigures()

GET         /fix/dedup                  controllers.TransactionController.dedup()
